{
  "kind": "collectionType",
  "collectionName": "news",
  "info": {
    "name": "News"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "blogContent": {
      "type": "richtext",
      "required": true,
      "unique": true
    },
    "featureImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "categories": {
      "via": "news",
      "collection": "category"
    }
  }
}
